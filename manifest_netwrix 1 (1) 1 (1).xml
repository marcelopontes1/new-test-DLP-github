<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
  <!-- Begin Basic Settings: Add-in metadata, used for all versions of Office unless override provided. -->
  <!-- IMPORTANT! Id must be unique for your add-in, if you reuse this manifest ensure that you change this id to a new GUID. -->
<!--
  <Id>6cd6c5d5-f2b8-4e35-b82e-f0ec51826084</Id>
-->
  <Id>9bd49bea-367d-4217-b64a-83088b10d491</Id>
  <!--Version. Updates from the store only get triggered if there is a version change. -->
  <Version>1.0.0.2</Version>
  <ProviderName>Netwrix</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <!-- The display name of your add-in. Used on the store and various places of the Office UI such as the add-ins dialog. -->
  <DisplayName DefaultValue="Netwrix Endpoint Protector Outlook web extension"/>
  <Description DefaultValue="Netwrix Endpoint Protector prevents confidential data leaks for your organization"/>
  <!-- <IconUrl DefaultValue="https://img.netwrix.com/products/icons/24/nwx-nep.svg"/> -->
  <HighResolutionIconUrl DefaultValue="https://images.g2crowd.com/uploads/product/image/large_detail/large_detail_f611cdcae8cdd8d4330e738974f57170/netwrix-endpoint-protector.png"/>
  <SupportUrl DefaultValue="https://www.endpointprotector.com"/>
  <!-- Domains that will be allowed when navigating. For example, if you use ShowTaskpane and then have an href link, navigation will only be allowed if the domain is on this list. -->
  <AppDomains>
    <AppDomain>localhost</AppDomain>
    <AppDomain>127.0.0.1</AppDomain>
    <AppDomain>::1</AppDomain>
    <AppDomain>endpointprotector.com</AppDomain>
    <AppDomain>download.endpointprotector.com</AppDomain>
  </AppDomains>
  <!--End Basic Settings. -->
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <!--Production link-->
        <SourceLocation DefaultValue="https://download.endpointprotector.com/validator/incercare.html"/>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteMailbox</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit"/>
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit"/>
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
      <Requirements>
        <bt:Sets DefaultMinVersion="1.1">
          <bt:Set Name="Mailbox"/>
        </bt:Sets>
      </Requirements>
      <Hosts>
        <Host xsi:type="MailHost">
          <DesktopFormFactor>
            <FunctionFile resid="residUILessFunctionFileUrl"/>
            <ExtensionPoint xsi:type="Events">
              <Event Type="ItemSend" FunctionExecution="synchronous" FunctionName="onMessageSendHandler"/>
            </ExtensionPoint>
          </DesktopFormFactor>
        </Host>
      </Hosts>
      <Resources>
        <bt:Urls>
          <!--Production link-->
          <bt:Url id="residUILessFunctionFileUrl" DefaultValue="https://download.endpointprotector.com/validator/incercare.html"/>
        </bt:Urls>
      </Resources>
    </VersionOverrides>
  </VersionOverrides>
</OfficeApp>
